# Randomness
seed -1

# Create
uniform_ratio 0.3
population_size 50

# Continue
min_generations 0
max_generations 500
stall_generations 50

# Select
tournament_size 3

# Crossover
crossover_rate 0.3
crossover_points 1

# Mutate
mutation_rate 0.005

# Evolve
elitism_rate 0.2

verbose 1
cache 0

dump_evolution evolution.txt
