# Randomness
seed 0

# Create
uniform_ratio 0.3
population_size 50

# Continue
min_generations 0
max_generations 10000
stall_generations 500

# Select
tournament_size 2

# Crossover
crossover_min_rate 0.4
crossover_scale 0
crossover_exponent 0
crossover_points 1

# Mutate
mutation_min_rate 0.005
mutation_scale 0.25
mutation_exponent -0.06

# Evolve
elitism_rate 1

verbose 1
cache 0
reorder_tasks 1

dump_evolution evolution.txt
