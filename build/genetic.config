# Randomness
seed 0

# Create
uniform_ratio 0.2
population_size 50

# Continue
min_generations 0
max_generations 500
stall_generations 20

# Select
tournament_size 2

# Crossover
crossover_rate 0.3
crossover_points 3

# Mutate
chromosome_mutation_rate 1
gene_mutation_rate 0.005

# Evolve
elitism_rate 0.2

verbose 1
cache 0

dump_evolution evolution.txt
