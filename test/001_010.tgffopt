# Task graph label
tg_label TASK_GRAPH
# Number of graphs
tg_cnt 1
# Number of tasks per graph +/- jitter
task_cnt 15 1
# Number of different tasks
task_type_cnt 15
# Average time per task including communication
task_trans_time 1
# Deadline jitter
deadline_jitter 0
# All tasks are different?
task_unique false
# Write
tg_write
eps_write

# Processing element (PE) label
table_label PE
# Number of PEs
table_cnt 1
# Attributes of PEs
table_attrib \
  frequency 1e9 0, \
  voltage 1 0, \
  ngate 2e5 0
# Attributes of tasks for particular PE
type_attrib \
  effective_switched_capacitance 6e-9 4e-9, \
  number_of_clock_cycles 30e6 20e6
# Write
pe_write
