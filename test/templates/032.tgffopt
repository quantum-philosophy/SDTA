# Randomize!
seed [SEED]

# Task graph label
tg_label TASK_GRAPH
# Number of graphs
tg_cnt 1
# Number of tasks per graph +/- jitter
task_cnt [TASK_COUNT] 0.001
# Number of different tasks
task_type_cnt [TYPE_COUNT]
# Average time per task including communication
task_trans_time 1
# Deadline jitter
deadline_jitter 0
# Have exactly the same task count each time
period_mul 1
# Number of in and out edges
task_degree 3 4
# All tasks are different?
task_unique false
# Write
tg_write
eps_write

# Processing element (PE) label
table_label PE
# Number of PEs
table_cnt [PROCESSOR_COUNT]
# Attributes of PEs
table_attrib \
  frequency 1000e6 200e6 0 50e6, \
  voltage 1 0.2 0 0.1, \
  ngate 7e4 0
# Attributes of tasks for particular PE
type_attrib \
  effective_switched_capacitance 4e-9 3e-9, \
  number_of_clock_cycles 11e6 9e6
# Write
pe_write
